players.on_join(function(pid)
    menu.divider(menu.player_root(pid), "Network Menu")
    local playerfunc = menu.list(menu.player_root(pid), ('Network Menu Player Functions'), {}, '' )
	menu.action(playerfunc, "Player Insanity", {"insanity"}, "", function() perm = COMMANDPERM_RUDE
            if pid == players.user() then 
                util.toast('[S.R.E] I just saved you from fucking your game. Thank me later :D')
                util.show_corner_help("~r~[S.R.E] ~w~I just saved you from fucking your game. Thank me later :D")
