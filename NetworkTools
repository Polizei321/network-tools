for _, pid in ipairs(players.list(true, true, true)) do
    if players.get_name(pid) == "zPolizei" then
        util.yield(1000)
        util.show_corner_help("A ~r~Network Menu Dev ~w~(" .. players.get_name(pid) .. ") is detected in your lobby.")
    end
end
